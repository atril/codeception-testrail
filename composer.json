{
    "name": "bookit/codeception-testrail",
    "description": "Codeception Extension for TestRail Reporting",
    "homepage": "https://github.com/bookitcom",
    "license": "MIT",
    "require": {
        "codeception/codeception": "^4.1",
        "phpunit/phpunit": "^7.5",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.6",
        "codeception/module-phpbrowser": "^2.0",
        "codeception/module-asserts": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BookIt\\Codeception\\TestRail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BookIt\\Codeception\\TestRail\\Tests\\Unit\\": "tests/unit"
        }
    }
}
